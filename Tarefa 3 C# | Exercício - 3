        static void Main(string[] args)
        {
            int[] valores = new int[99];

            Console.WriteLine("ENTRE COM 99 VALORES: ");
            for (int i = 0; i < 99; i++)
            {
                valores[i] = int.Parse(Console.ReadLine());
            }
            Console.WriteLine("INVERTIDOS FICAM: ");
            for (int i = 98; i >= 0; i--)
            {
                Console.WriteLine(valores[i] + " ");
            }
            Console.ReadKey();
        }
