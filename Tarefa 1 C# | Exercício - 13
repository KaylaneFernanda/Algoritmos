        static void Main(string[] args)
        {
            double num;
            Console.Write("ENTRE COM UM VALOR NÃšMERICO: ");
            num = double.Parse(Console.ReadLine());

            if (num > 100 && num < 200)
            {
                Console.WriteLine("O VALOR {0} ESTÃ ENTRE 1OO E 200", num);
            }
            else
            {
                Console.WriteLine("O VALOR {0} NÃƒO ESTÃ ENTRE 100 E 200", num);
            }
            Console.ReadKey();
        }
