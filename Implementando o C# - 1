static void Main(string[] args)
 {
 double[] array = new double[15];
 double[] cubo = new double[15];
 int j=0, i;
 Console.WriteLine("ENTRE COM 15 VALORES: ");
 for (i=0; i<15; i++)
 {
 array[i] = double.Parse(Console.ReadLine());
 }
 for (i=0; i<15; i++)
 {
 cubo[j] = array[i] * array[i] * array[i];
 Console.WriteLine("CUBO DOS VALORES ENTRADOS RESPECTIVAMENTE: " + cubo[j]);
 array[i]++;
 }
 Console.ReadKey();
 }
